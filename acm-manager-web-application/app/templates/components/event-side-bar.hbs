<div id="upcoming-events" class="upcoming-events__expanded panel panel-info">
  <div class="panel-heading">
    <h3 class="panel-title">Upcoming Events</h3>
  </div>

  <div class="panel-body">
    Sort event preview by:<br>
    <div class="btn-group" role="group" aria-label="">
      <button disabled="disabled" type="button" class="btn btn-xs btn-default">Time</button>
      <button type="button" class="btn btn-xs btn-default">Type</button>
    </div>
  </div>

  <div class="list-group upcoming-events-list no-scroll-bar" data-spy="scroll">  {{#each-in eventData as |key value|}}
      {{list-item-event data=value}}
    {{/each-in}}
  </div>
</div>

{{yield}}
